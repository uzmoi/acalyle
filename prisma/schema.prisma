datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Book {
    id        String   @id
    thumbnail String
    title     String
    createdAt DateTime
    memos     Memo[]
}

model Memo {
    id        String   @id
    contents  String
    createdAt DateTime
    updatedAt DateTime
    Book      Book     @relation(fields: [bookId], references: [id])
    bookId    String
}
