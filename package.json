{
  "private": true,
  "name": "acalyle",
  "type": "module",
  "version": "0.1.0",
  "workspaces": [
    "renderer",
    "main",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "check": "turbo run check",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "coverage": "turbo run coverage",
    "format": "prettier -w \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "electron": "^23.0.0",
    "eslint": "^8.38.0",
    "jsonc-parser": "^3.2.0",
    "prettier": "^2.8.7",
    "turbo": "^1.8.3",
    "typescript": "^5.0.4"
  }
}
