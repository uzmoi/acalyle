{
  "private": true,
  "name": "acalyle",
  "type": "module",
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "check": "turbo run check",
    "prelint": "npm run build -w @acalyle/eslint-config",
    "lint": "eslint .",
    "test": "turbo run test",
    "coverage": "turbo run coverage",
    "format": "prettier -w \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^8.50.0",
    "electron": "^23.0.0",
    "eslint": "^8.50.0",
    "globals": "^13.22.0",
    "prettier": "^2.8.7",
    "turbo": "^1.8.3",
    "typescript": "^5.2.2"
  }
}
